# The default verbosity level
set verbosity 11

# Print debug info about the ActionScript classes as they are processed
set actionDump off

# Print debug info about how Gnash is parsing the file
set parserDump off

# The full path to the debug log
set debuglog /tmp/cygnal-dbg.log

# The Port offset to use when we're not root
set portOffset 4000

# The Document root where multimedia files live
set documentroot /var/www/html

# Don't start an admin thread
set admin no

# Do use a fully threaded server
set threading yes

# Do use a fully threaded server
set testing yes

# When running in threaded mode, this is the number of file descritors
# watched by each thread
set fdThread 10

# Turn on debugging for network layer
set netdebug no
